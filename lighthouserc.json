{
  "ci": {
    "collect": {
      "startServerCommand": "npm run build && npm run server",
      "url": ["http://localhost:3000/"],
      "numberOfRuns": 3
    },
    "assert": {
      "assertions": {
        "first-contentful-paint": ["warn", { "maxNumericValue": 1000 }],
        "largest-contentful-paint": ["warn", { "maxNumericValue": 1000 }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
